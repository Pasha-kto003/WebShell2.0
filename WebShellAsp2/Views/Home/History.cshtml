@model IEnumerable<WebShellAsp2.Models.CommandHistory>

@{
    ViewData["Title"] = "CommandHistoryView";
}

<h1 style="color:blue">История</h1>
<table class="resp-tab">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Command.CommandName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateUsing)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CommandAnswer)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Command.CommandName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateUsing)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CommandAnswer)
            </td>
            <td>
                <a asp-action="DetailsView" asp-controller="History" asp-route-id="@item.ID">Подробнее</a>
            </td>
        </tr>
}
    </tbody>
</table> 
